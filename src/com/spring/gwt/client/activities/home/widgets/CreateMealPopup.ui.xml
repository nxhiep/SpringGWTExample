		</div>
		<div class="row">
			<div class="col-sm-12 col-md-4">Mô tả</div>
			<div class="col-sm-12 col-md-8">
				<g:TextArea ui:field="descriptionBox"></g:TextArea>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 